{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["src/components/*"],
      "modules/*": ["src/modules/*"],
      "blocks/*": ["src/blocks/*"],
      "layouts/*": ["src/layouts/*"],
      "assets/*": ["src/assets/*"]
    }
  }
}
